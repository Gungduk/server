<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="search">
    <select id="searchId" parameterType="String" resultType="java.util.String">
        SELECT 	email
        FROM 	user
        WHERE	phoneNum = #{phoneNum}
    </select>
    
    <select id="searchPw" parameterType="String" resultType="java.util.String">
        SELECT 	pw
        FROM 	user
        WHERE	email = #{email}
    </select>
</mapper>